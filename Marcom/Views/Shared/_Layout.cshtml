                    <ul class="section menu">
                        <li><a class="menuitem">Master Data</a>
                            <ul class="submenu">
                                <li><a href="@Url.Action("index","Brands")">Brands</a> </li>
                                <li><a href="@Url.Action("index","Departments")">Product Range</a> </li>
                                <li><a href="@Url.Action("index","Categorys")">Categories</a> </li>
                              
                            </ul>
                        </li>
                        <li><a class="menuitem">Company Data</a>
                            <ul class="submenu">
                                <li><a href="@Url.Action("index", "CompanyProfile")">Company Profile</a> </li>
                                <li><a href="@Url.Action("index", "News")">Company News</a> </li>
                                <li><a href="@Url.Action("index", "Event")">Company Events</a> </li>
                            </ul>
                        </li>
                        <li><a class="menuitem">Product Data</a>
                            <ul class="submenu">
                                <li><a href="@Url.Action("index", "Product")">Product</a> </li>
                            </ul>
                        </li>
                        <li><a class="menuitem">Weekly Offer</a>
                            <ul class="submenu">
                                <li><a href="@Url.Action("index", "WeeklyOffer")">Offer</a> </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="grid_10">
              @RenderBody()
        </div>
        
        
        <div class="clear">
        </div>
