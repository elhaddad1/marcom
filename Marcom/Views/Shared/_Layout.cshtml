<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")")" rel="stylesheet" type="text/css" />
    
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/css/reset.css")" media="screen" />
